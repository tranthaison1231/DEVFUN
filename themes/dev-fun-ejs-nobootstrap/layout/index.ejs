<div>
    <ul>
    <% page.posts.each((post) =>{ %>
        <li>
        <h3>
            <a href="<%- url_for(post.path) %>">
                <%= post.title %>
            </a>
        </h3>
        <div>
            <%= date(post.date, config.date_format) %>
        </div>
        <% if(post.excerpt) { %>
            <p>
                <%- post.excerpt %>
            </p>
        <% } %>
        </li>
    <% }) %>
    </ul>
</div>
